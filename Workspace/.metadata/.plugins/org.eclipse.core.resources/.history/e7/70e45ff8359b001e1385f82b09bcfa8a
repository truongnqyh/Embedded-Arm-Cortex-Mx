#ifndef MAIN_H_
#define MAIN_H_

/* Systick clock */
#define SYSTICK_TIM_CLK 16000000U

/* Stack allocation */
#define SRAM_END 0x3FFFFFFFU;
#define NUM_OF_KB_EACH_TASK 1U;
#define T1_STACK_START (SRAM_END - (0 * 1024U))
#define T2_STACK_START (SRAM_END - (0 * 1024U))
#define T3_STACK_START (SRAM_END - (0 * 1024U))
#define T4_STACK_START (SRAM_END - (0 * 1024U))
#define SCHED_STACK_START
#endif /* MAIN_H_ */
